{
  "editor.formatOnSave": true,
  "prettier.eslintIntegration": true,
  "movets.skipWarning": true,
  "typescriptHero.imports.organizeOnSave": true,
  "typescriptHero.imports.multiLineTrailingComma": true,
  "typescriptHero.imports.insertSpaceBeforeAndAfterImportBraces": true,
  "typescriptHero.imports.multiLineWrapThreshold": 100,
  "xmlTools.splitXmlnsOnFormat": false,
  "files.eol": "\n",
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "html"],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".cache-loader": true,
    "node_modules": true,
    "coverage": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "markdown": "javascriptreact"
  },
  "emmet.excludeLanguages": [],
  "beautify.ignore": ["*"],
  "[html]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false
  },
  "editor.formatOnSaveTimeout": 3000
}
